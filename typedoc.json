{
    "$schema": "https://typedoc.org/schema.json",
    "entryPoints": [
        "./src"
    ],
    "entryPointStrategy": "expand",
    "out": "docs",
    "plugin": [
        "typedoc-plugin-vue"
    ],
    "exclude": [
        "**/*.spec.ts",
        "**/*.d.ts"
    ],
    "excludePrivate": false,
    "excludeProtected": false,
    "excludeInternal": false,
    "excludeExternals": true,
    "excludeNotDocumented": true,
    "includeVersion": true,
    "categorizeByGroup": true,
    "categoryOrder": [
        "Components",
        "Services",
        "Types",
        "Utils",
        "*"
    ],
    "name": "Amazing List Documentation",
    "readme": "README.md",
    "hideGenerator": true,
    "validation": {
        "notExported": true,
        "invalidLink": true,
        "notDocumented": true
    },
    "requiredToBeDocumented": [
        "Class",
        "Interface",
        "Function",
        "Method"
    ],
    "visibilityFilters": {
        "protected": true,
        "private": true,
        "inherited": true,
        "external": true
    }
}