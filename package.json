{
  "name": "amazing-list-app",
  "author": "Ilia Ruchevskii <helloris2591@gmail.com>",
  "license": "MIT",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "docs": "typedoc",
    "docs:dev": "nodemon --watch src -e js,ts,vue --exec npx typedoc",
    "typecheck": "vue-tsc --noEmit",
    "typecheck:watch": "vue-tsc --noEmit --watch",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --fix",
    "lint:check": "eslint . --ext .vue,.js,.ts,.jsx,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,vue,js,jsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,vue,js,jsx}\"",
    "prepare": "husky"
  },
  "dependencies": {
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-vue": "^9.19.0",
    "husky": "^9.1.7",
    "nodemon": "^3.0.2",
    "prettier": "^3.4.2",
    "typedoc": "^0.25.7",
    "typedoc-plugin-vue": "^1.1.0",
    "typescript": "~5.3.3",
    "vite": "^5.0.0",
    "vue-tsc": "^1.8.27"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=7.0.0"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "not dead",
    "not IE 11"
  ]
}
